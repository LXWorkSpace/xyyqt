/* miniprogram/pages/my/cxsq/cxsq.wxss */
.cont{
  display: flex;
  align-items: center;
  flex-direction: column;
  background-color: #E3F8F3;
  border-bottom:1px solid #DDDDDD;
  border-top:1px solid #DDDDDD;
  padding-bottom: 60px;

}

.img{
  width: 50px;
  margin: 40px 0px 20px 0px;
}

.res{
  background-color: #47D1AF;
  color: white;
  height: 40px;
  width: 150px;
  text-align: center;
  font-size: 18px;
  line-height: 40px;
  margin-bottom: 30px;
  font-weight: 700;
}


.v1{

  margin-right: 3px;
  border-top-left-radius: 20px;
  border-bottom-left-radius: 20px;
  
}
.v2{
  margin-right: 3px;
}
.v3{
  border-top-right-radius: 20px;
  border-bottom-right-radius: 20px;
}
.border{
  border: 1px solid #47D1AF;
  height: 6px;
  width: 90px;
  margin-bottom: 15px;
}

.progress{
  display: flex;
  justify-content: center;
  margin-top: 20px;
  font-size: 12px;
  color: #666666;
  text-align: center;
}


.full{
  background-color: #47D1AF;
}

.backBtn{
  background-color: white;
  width: 68%;
  margin-top: 35px;
  color: #66D9BD;
  font-weight: 700;
  border-radius: 50px;
  border: 1px solid #66D9BD;
}